	"styles": {
		"blocks": {
			"core/code": {
				"border": {
					"radius": "6px"
				},
				"color": {
					"background": "var(--wp--preset--color--light-grey-blue)",
					"text": "var(--wp--preset--color--near-black)"
				},
				"spacing": {
					"padding": {
						"bottom": "var(--wp--preset--spacing--small)",
						"left": "var(--wp--preset--spacing--small)",
						"right": "var(--wp--preset--spacing--small)",
						"top": "var(--wp--preset--spacing--small)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--nunito-sans)"
				}
			},
			"core/preformatted": {
				"border": {
					"radius": "6px"
				},
				"color": {
					"background": "var(--wp--preset--color--light-grey-blue)",
					"text": "var(--wp--preset--color--near-black)"
				},
				"spacing": {
